{
  "extends": [
    "universe/native",
    "universe/shared/typescript-analysis"
  ],
  "overrides": [
    {
      "files": [
        "*.ts",
        "*.tsx",
        "*.d.ts"
      ],
      "parserOptions": {
        "project": "./tsconfig.json"
      }
    }
  ],
  "rules": {
    "import/order": "error",
    "@typescript-eslint/no-unused-vars": "error"
  }
}
